<p id="notice"><%= notice %></p>

<h1>Type Jobs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Skilsrequired</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_jobs.each do |type_job| %>
      <tr>
        <td><%= type_job.name %></td>
        <td><%= type_job.skilsRequired %></td>
        <td><%= link_to 'Show', type_job %></td>
        <td><%= link_to 'Edit', edit_type_job_path(type_job) %></td>
        <td><%= link_to 'Destroy', type_job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Job', new_type_job_path %>
