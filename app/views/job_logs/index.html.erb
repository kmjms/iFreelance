<p id="notice"><%= notice %></p>

<h1>Job Logs</h1>

<table>
  <thead>
    <tr>
      <th>State code</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_logs.each do |job_log| %>
      <tr>
        <td><%= job_log.state_code %></td>
        <td><%= job_log.message %></td>
        <td><%= link_to 'Show', job_log %></td>
        <td><%= link_to 'Edit', edit_job_log_path(job_log) %></td>
        <td><%= link_to 'Destroy', job_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Log', new_job_log_path %>
