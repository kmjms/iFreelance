<p id="notice"><%= notice %></p>

<h1>Quotation Registries</h1>

<table>
  <thead>
    <tr>
      <th>Idquotationregistry</th>
      <th>Date</th>
      <th>Nameclient</th>
      <th>Business</th>
      <th>Product</th>
      <th>Value</th>
      <th>Validity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotation_registries.each do |quotation_registry| %>
      <tr>
        <td><%= quotation_registry.idquotationregistry %></td>
        <td><%= quotation_registry.date %></td>
        <td><%= quotation_registry.nameclient %></td>
        <td><%= quotation_registry.business %></td>
        <td><%= quotation_registry.product %></td>
        <td><%= quotation_registry.value %></td>
        <td><%= quotation_registry.validity %></td>
        <td><%= link_to 'Show', quotation_registry %></td>
        <td><%= link_to 'Edit', edit_quotation_registry_path(quotation_registry) %></td>
        <td><%= link_to 'Destroy', quotation_registry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quotation Registry', new_quotation_registry_path %>
